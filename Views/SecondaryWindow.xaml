<Window x:Class="BidCardCoin.Vue.SecondaryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:BidCardCoin.Vue"
        mc:Ignorable="d"
        Title="SecondaryWindow" Height="450" Width="800">
    <Grid >
         
        <StackPanel Name="NavBar" Orientation="Horizontal" VerticalAlignment="Top" 
                    HorizontalAlignment="Center" StackPanel.ZIndex="1">
            <Button Click="btnEnchere" Height="40" Width="75">Voir Enchere</Button>
            <Button Click="btnLot" Height="40" Width="75">Voir Lot</Button>
            <Button Click="btnProduit" Height="40" Width="75">Voir produit</Button>
        </StackPanel>
        <StackPanel Name="stkTest" >
            <Label Content="Secondary Window" StackPanel.ZIndex="0" ></Label>  
        </StackPanel>
        
        
    </Grid>
</Window>
